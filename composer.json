{
    "name": "zelvad/nova-external-url",
    "description": "An external URL Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Zelvad\\ExternalUrl\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zelvad\\ExternalUrl\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
